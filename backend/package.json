{
  "name": "codeskins-backend",
  "version": "1.0.0",
  "description": "CodeSkins Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "fastify": "4",
    "@fastify/cors": "8",
    "@fastify/helmet": "11",
    "@fastify/jwt": "7",
    "@fastify/multipart": "8",
    "@fastify/static": "6",
    "mongoose": "8",
    "bcryptjs": "2",
    "jsonwebtoken": "9",
    "stripe": "14",
    "dotenv": "16",
    "zod": "3",
    "multer": "1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "@types/jest": "^30.0.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.0"
  }
}
